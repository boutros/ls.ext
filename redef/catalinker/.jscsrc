{
  "preset": "crockford",
  "maximumLineLength": {
    "value": 180,
    "tabSize": 2,
    "allExcept": ["urlComments", "regex"]
  },
  "requireVarDeclFirst": null,
  "validateIndentation": 2,
  "requireMultipleVarDecl": null,
  "disallowDanglingUnderscores": false,
  "excludeFiles": [
    "node_modules"
  ],
  "esversion": 6
}